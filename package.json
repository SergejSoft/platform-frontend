  "scripts": {
    "build:prod": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "ganache": "./scripts/ganache-cli.sh",
    "start": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "format": "prettier --list-different '**/*.{ts,tsx,js,json,md, gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,json,md,gql}'",
    "tslint": "tslint --project . --format stylish && tslint --project . --format stylish --config ./tslint.spec.json '**/*.spec.ts?(x)'",
    "tslint:fix": "tslint --fix --project . --format stylish && tslint --fix --project . --format stylish --config ./tslint.spec.json '**/*.spec.ts?(x)' '**/*.spec.node.ts?(x)'",
    "tsc": "tsc --noEmit --project .",
    "sasslint": "sass-lint -c .sass-lint.yml -v -q",
    "lint": "yarn format && yarn tslint && yarn sasslint && yarn tsc",
    "lint:fix": "yarn tslint:fix && yarn format:fix && yarn sasslint && yarn tsc",
    "test:browser": "NODE_ENV=test mocha --require ts-node/register --require jsdom-global/register --require ./test/setupBrowserTests.ts './test/setupTestsHooks.ts' '**/*.spec.ts?(x)'",
    "test:browser-coverage": "NODE_ENV=test nyc mocha --require ts-node/register --require jsdom-global/register --require ./test/setupBrowserTests.ts 'test/setupTestsHooks.ts' '**/*.spec.ts?(x)'",
    "test:node": "NODE_ENV=test mocha --require ts-node/register --require ./test/setupTests.ts '**/*.spec.node.ts?(x)'",
    "test:node-coverage": "NODE_ENV=test nyc mocha --require ts-node/register --require ./test/setupTests.ts '**/*.spec.node.ts?(x)'",
    "test": "yarn lint && yarn test:browser && yarn test:node",
    "test:coverage": "yarn lint && yarn test:browser-coverage && yarn test:node",
    "test:fix": "yarn lint:fix && yarn test:browser && yarn test:node",
    "codecov": "codecov",
    "generate:contracts": "typechain --force --outDir app/lib/contracts './node_modules/neufund-contracts-tmp/abis/*.json'",
    "prepare": "yarn generate:contracts"
  },
